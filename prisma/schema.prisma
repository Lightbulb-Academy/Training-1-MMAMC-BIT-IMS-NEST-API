generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Role {
  @@map("roles")

  id Int @id @default(autoincrement())
  name String @db.VarChar(50) @unique
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}
